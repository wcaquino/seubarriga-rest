{
  "name": "seubarriga",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "directories": {
    "test": "test"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "branches": 80,
        "functions": 80,
        "lines": 80
      },
      "./src/services": {
        "lines": 100
      }
    }
  },
  "scripts": {
    "start": "NODE_ENV=prod node src/server.js",
    "test": "jest --coverage --runInBand --forceExit",
    "lint": "eslint src/** test/** --fix",
    "secure-mode": "NODE_ENV=test jest --watch --verbose=true"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint &&npm test"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
